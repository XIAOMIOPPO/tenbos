package com.yidu.tencloud.bean.basis.regionsetting;

import java.io.Serializable;

/**
 * 区域类
 * @author 鲁静芝
 * @since 2017/11/6
 */
public class Area implements Serializable {

	/**
	 * 序列号
	 */
	private static final long serialVersionUID = 6595663532319294948L;
	/**
	 * 区域ID
	 */
	private String areaId;
	/**
	 * 省份
	 */
	private String province;
	/**
	 * 城市
	 */
	private String city;
	/**
	 * 县级
	 */
	private String county;
	/**
	 * 邮编
	 */
	private String postcode;
	/**
	 * 城市编码
	 */
	private String citycode;
	/**
	 * 简码
	 */
	private String brevitycode;
	/**
	 * 状态
	 */
	private String isUse;
	/**
	 * 获得区域Id
	 * @return 区域ID
	 */
	public String getAreaId() {
		return areaId;
	}
	/**
	 * 设置区域ID
	 * @param areaId 区域ID
	 */
	public void setAreaId(String areaId) {
		this.areaId = areaId;
	}
	/**
	 * 获得省份
	 * @return 省份
	 */
	public String getProvince() {
		return province;
	}
	/**
	 * 设置省份
	 * @param province 省份
	 */
	public void setProvince(String province) {
		this.province = province;
	}
	/**
	 * 获得城市
	 * @return 城市
	 */
	public String getCity() {
		return city;
	}
	/**
	 * 设置城市
	 * @param city 城市
	 */
	public void setCity(String city) {
		this.city = city;
	}
	/**
	 * 获得县级
	 * @return 县级
	 */
	public String getCounty() {
		return county;
	}
	/**
	 * 设置县级
	 * @param county 县级
	 */
	public void setCounty(String county) {
		this.county = county;
	}
	/**
	 * 获得邮编
	 * @return 邮编
	 */
	public String getPostcode() {
		return postcode;
	}
	/**
	 * 设置邮编
	 * @param postcode 邮编
	 */
	public void setPostcode(String postcode) {
		this.postcode = postcode;
	}
	/**
	 * 获得城市编码
	 * @return 城市编码
	 */
	public String getCitycode() {
		return citycode;
	}
	/**
	 * 设置城市编码
	 * @param citycode 城市编码
	 */
	public void setCitycode(String citycode) {
		this.citycode = citycode;
	}
	/**
	 * 获得简码
	 * @return 简码
	 */
	public String getBrevitycode() {
		return brevitycode;
	}
	/**
	 * 设置简码
	 * @param brevitycode 简码
	 */
	public void setBrevitycode(String brevitycode) {
		this.brevitycode = brevitycode;
	}
	/**
	 * 获得状态
	 * @return 状态
	 */
	public String getIsUse() {
		return isUse;
	}
	/**
	 * 设置状态
	 * @param isUse 状态
	 */
	public void setIsUse(String isUse) {
		this.isUse = isUse;
	}
	
	
	/**
	 * 有参的构造方法
	 * @param areaId 区域ID
	 * @param province 省份
	 * @param city 城市
	 * @param county 县级
	 * @param postcode 邮编
	 * @param citycode 城市编码
	 * @param brevitycode 简码
	 * @param status 状态
	 */
	public Area(String areaId, String province, String city, String county, String postcode, String citycode,
			String brevitycode, String isUse) {
		super();
		this.areaId = areaId;
		this.province = province;
		this.city = city;
		this.county = county;
		this.postcode = postcode;
		this.citycode = citycode;
		this.brevitycode = brevitycode;
		this.isUse = isUse;
	}
	
	/**
	 * 无参的构造方法
	 */
	public Area() {
		
	}
	
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((areaId == null) ? 0 : areaId.hashCode());
		result = prime * result + ((brevitycode == null) ? 0 : brevitycode.hashCode());
		result = prime * result + ((city == null) ? 0 : city.hashCode());
		result = prime * result + ((citycode == null) ? 0 : citycode.hashCode());
		result = prime * result + ((county == null) ? 0 : county.hashCode());
		result = prime * result + ((isUse == null) ? 0 : isUse.hashCode());
		result = prime * result + ((postcode == null) ? 0 : postcode.hashCode());
		result = prime * result + ((province == null) ? 0 : province.hashCode());
		return result;
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Area other = (Area) obj;
		if (areaId == null) {
			if (other.areaId != null)
				return false;
		} else if (!areaId.equals(other.areaId))
			return false;
		if (brevitycode == null) {
			if (other.brevitycode != null)
				return false;
		} else if (!brevitycode.equals(other.brevitycode))
			return false;
		if (city == null) {
			if (other.city != null)
				return false;
		} else if (!city.equals(other.city))
			return false;
		if (citycode == null) {
			if (other.citycode != null)
				return false;
		} else if (!citycode.equals(other.citycode))
			return false;
		if (county == null) {
			if (other.county != null)
				return false;
		} else if (!county.equals(other.county))
			return false;
		if (isUse == null) {
			if (other.isUse != null)
				return false;
		} else if (!isUse.equals(other.isUse))
			return false;
		if (postcode == null) {
			if (other.postcode != null)
				return false;
		} else if (!postcode.equals(other.postcode))
			return false;
		if (province == null) {
			if (other.province != null)
				return false;
		} else if (!province.equals(other.province))
			return false;
		return true;
	}
	@Override
	public String toString() {
		return "Area [areaId=" + areaId + ", province=" + province + ", city=" + city + ", county=" + county
				+ ", postcode=" + postcode + ", citycode=" + citycode + ", brevitycode=" + brevitycode + ", isUse="
				+ isUse + "]";
	}
	
	


}
